{
  "name": "@am-ogs/vue-ui",
  "version": "0.0.15",
  "description": "Library for components that are re-used across all my projects,",
  "author": "afshan.aman@gmail.com",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-ui.umd.js",
  "module": "./dist/vue-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-ui.es.js",
      "require": "./dist/vue-ui.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/am-my-portfolio/vue-components-library.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.vue\" \"**/*.mjs\"",
    "lint:fix": "eslint --fix \"**/*.js\" \"**/*.ts\" \"**/*.vue\" \"**/*.mjs\""
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@heroicons/vue": "^2.2.0",
    "@iconify/vue": "^4.3.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/lodash": "^4.17.16",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^13.1.0",
    "typescript": "^5.8.3",
    "vue-router": "^4.4.3",
    "lodash": "^4.17.21",
    "vite": "^6.2.0",
    "path": "^0.12.7",
    "@types/node": "^22.14.1"
  },
  "keywords": [
    "vue",
    "tailwind4"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/am-my-portfolio/vue-components-library/issues"
  },
  "homepage": "https://github.com/am-my-portfolio/vue-components-library#readme"
}
